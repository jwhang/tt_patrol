{
	"manifest_version": 3,
	"name": "TT Patrol",
	"version": "1.2",
	"description": "Enforces Travel Tuesday rules",
	"content_scripts": 
	[
		{
			"matches": ["*://*.messenger.com/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"background": {
		"service_worker": "background.js"
	},
	"host_permissions": [
		"*://172.232.7.65:*/*"
	]

}
